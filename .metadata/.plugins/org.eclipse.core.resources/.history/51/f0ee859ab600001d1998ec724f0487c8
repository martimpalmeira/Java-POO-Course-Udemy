package exercises.secao19.practice231.application;

public class Program {

	public static void main(String[] args) {
		double x = 2147483647;
		System.out.println(fatorial(x));
		long y = (long)Math.pow(2, 31);
		System.out.print(y);
	}

	public static int fatorial(double n) {
		int x = 0;
		while (n > 2) {
			n /= 2;
			x += 1;
		}
		
		return x;
	}
}
